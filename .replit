entrypoint = "index.js"
modules = ["nodejs-22"]
hidden = [".config", "package-lock.json"]

[gitHubImport]
requiredFiles = [".replit", "replit.nix", "package.json", "package-lock.json"]

[nix]
channel = "stable-24_11"

[deployment]
run = ["node", "index.js"]
deploymentTarget = "autoscale"
ignorePorts = false

[workflows]
runButton = "Lancer Agent IA"

[[workflows.workflow]]
name = "run package"
mode = "sequential"
author = 10543762

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "node index.js"

[[workflows.workflow]]
name = "install"
mode = "sequential"
author = 10543762

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm install express path"

[[workflows.workflow]]
name = "Web ia"
mode = "sequential"
author = 10543762

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "node crocobras-agent.js"
